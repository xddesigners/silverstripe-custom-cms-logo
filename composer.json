{
    "name": "xddesigners/silverstripe-custom-cms-logo",
    "description": "Allows you to customize CMS logo.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/symbiote/silverstripe-custom-cms-logo",
    "keywords": ["silverstripe", "logo", "custom", "cms"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Remy Vaartjes",
            "homepage": "https://github.com/rvxd"
        }
    ],
    "require": {
        "silverstripe/admin": "^1.3 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "XD\\CustomCMSLogo\\\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev"
}
